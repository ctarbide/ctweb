=all
,.PHONY: all
,all: $(C_PROGRAMS)
@

=realpath
type c-program
dependency config.h
dependency strlcat.c
dependency strlcpy.c
dependency realpath.c
dependency realpath-main.c
@

=clean
,.PHONY: clean
,clean:
,	@$(RM) *.$(OBJEXT) *-all.c
,	@$(RM) $(C_PROGRAMS)
,	@$(RM) $(NOFAKE_CHUNKS)
,	@$(RM) $(BUILD_MAKEFILE)
@
