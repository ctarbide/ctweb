
=all
,.PHONY: all
,all: all-src
@

=install
,.PHONY: install
,install: install-src
@

=all-src
,.PHONY: all-src
,all-src:
,	$(MAKE) -C src all
@

=install-src
,.PHONY: install-src
,install-src:
,	$(MAKE) -C src install
@

=update-Makefiles
,.PHONY: update-Makefiles
,update-Makefiles:
,	cd '$(TOP)' && MAKE='$(MAKE)' '$(VPATH_FROM_TOP)/build.sh'
@
