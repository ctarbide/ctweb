=all
,.PHONY: all
,all: $(NOFAKE_CHUNKS)
@

=clean
,.PHONY: clean
,clean:
,	$(RM) *.$(OBJEXT) *-all.c $(C_PROGRAMS) $(NOFAKE_CHUNKS)
@

=notangle
type nofake
source notangle.nw
chunk notangle notangle
target-option notangle executable 1
@

=noweave
type nofake
source noweave.nw
chunk noweave noweave
target-option noweave executable 1
@

=noweb
type nofake
source noweb.nw
chunk noweb noweb
target-option noweb executable 1
@

=roff
type nofake
source roff.nw
chunk noroff noroff
target-option noroff executable 1
@

=toroff
type nofake
source toroff.nw
chunk toroff toroff
target-option toroff executable 1
@
