
=all
,.PHONY: all
,all: $(NOFAKE_CHUNKS)
@

=toascii
type nofake
source toascii.nw
chunk toascii toascii
target-option toascii executable 1
@

=noxref
type nofake
source noxref.nw
chunk * noxref
target-option noxref executable 1
@

=update-Makefiles
,.PHONY: update-Makefiles
,update-Makefiles:
,	cd '$(TOP)' && MAKE='$(MAKE)' '$(VPATH_FROM_TOP)/build.sh'
@

=clean
,.PHONY: clean
,clean:
,	$(RM) *.$(OBJEXT) $(GENERATED_TARGETS)
@
