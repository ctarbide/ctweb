
=all
,.PHONY: all
,all: $(NOFAKE_CHUNKS)
@

=toascii
type nofake
source toascii.nw
chunk toascii toascii
target-option toascii executable 1
@

=noxref
type nofake
source noxref.nw
chunk * noxref
target-option noxref executable 1
@

=clean
,.PHONY: clean
,clean:
,	$(RM) *.$(OBJEXT) $(C_PROGRAMS) $(NOFAKE_CHUNKS)
@
