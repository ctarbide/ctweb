
=local-vars
,NOFAKEFLAGS = -L
@

=all
,.PHONY: all
,all: $(NOFAKE_CHUNKS) $(C_PROGRAMS)
@

=strsave.c
type nofake
source strsave.nw
chunk header strsave.h
chunk * strsave.c
@

=nwmktemp.c
type nofake
source nwmktemp.nw
chunk * nwmktemp.c
@

=match.c
type nofake
source match.nw
chunk header match.h
chunk * match.c
@

