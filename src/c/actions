=all
,.PHONY: all
,all: $(NOFAKE_CHUNKS)
@

=strsave
type nofake
source strsave.nw
chunk header strsave.h
chunk * strsave.c
@

=clean
,.PHONY: clean
,clean:
,	@$(RM) *.$(OBJEXT) *-all.c
,	@$(RM) $(C_PROGRAMS)
,	@$(RM) $(NOFAKE_CHUNKS)
,	@$(RM) $(BUILD_MAKEFILE)
@
