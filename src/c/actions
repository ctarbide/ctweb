dependency errors
dependency columns
dependency main.c
@

=nwmktemp
type c-program
dependency nwmktemp.c
@

=clean
,.PHONY: clean
,clean:
,	$(RM) *.$(OBJEXT) *-all.c $(C_PROGRAMS) $(NOFAKE_CHUNKS)
