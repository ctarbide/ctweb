@

=nt
type c-program
dependency notangle
dependency getline
dependency match
dependency modules
dependency modtrees
dependency strsave
dependency errors
dependency columns
source main.c
@

=nwmktemp
type c-program
source nwmktemp.c
@

=clean
,.PHONY: clean
,clean:
,	$(RM) *.$(OBJEXT) $(C_PROGRAMS) $(NOFAKE_CHUNKS)
