
=all
,.PHONY: all
,all: $(NOFAKE_CHUNKS)
@

=clean
,.PHONY: clean
,clean:
,	$(RM) *.$(OBJEXT) *-all.c $(C_PROGRAMS) $(NOFAKE_CHUNKS)
@

=multlang
type nofake
source multlang.nw
chunk nwmac.tex nwmac-LANGUAGE.tex
chunk noweb.sty noweb-LANGUAGE.sty
@

=support
type nofake
source support.nw
chunk nwmac.tex nwmac.tex
chunk noweb.sty noweb.sty
@

