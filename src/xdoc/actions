
=all
,.PHONY: all
,all: $(NOFAKE_CHUNKS)
@

=clean
,.PHONY: clean
,clean:
,	$(RM) *.$(OBJEXT) *-all.c $(C_PROGRAMS) $(NOFAKE_CHUNKS)
@

=docdate
type nofake
source docdate.nw
,# note: only for inclusion/prepend
,# required by:
,#     htmltoc.nw
,#     manpage.nw
,#     nodefs.nw
,#     noindex.nw
,#     noroff.nw
,#     noroots.nw
,#     nowebfilters.nw
,#     sl2h.nw
@

=htmltoc
type nofake
source htmltoc.nw
source docdate.nw
chunk * htmltoc.1
@

=noweb
type nofake
source noweb.nw
source docdate.nw
source manpage.nw
chunk noweb.1 noweb.1
@

=notangle
type nofake
source notangle.nw
source docdate.nw
source manpage.nw
source ../shell/noweave.nw
chunk notangle.1 notangle.1
@

=nodefs
type nofake
source nodefs.nw
source docdate.nw
chunk * nodefs.1
@

=noindex
type nofake
source noindex.nw
source docdate.nw
chunk * noindex.1
@

=noroff
type nofake
source noroff.nw
source docdate.nw
chunk * noroff.1
@

=noroots
type nofake
source noroots.nw
source docdate.nw
chunk * noroots.1
@

=nowebfilters
type nofake
source nowebfilters.nw
source docdate.nw
chunk * nowebfilters.1
@

=sl2h
type nofake
source sl2h.nw
source docdate.nw
chunk * sl2h.1
@

